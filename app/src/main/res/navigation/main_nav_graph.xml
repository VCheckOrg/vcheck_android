<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/demoStartFragment">

    <fragment
        android:id="@+id/livenessInstructionsFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.LivenessInstructionsFragment"
        android:label="fragment_main"
        tools:layout="@layout/liveness_instructions_fragment">
        <action
            android:id="@+id/action_mainFragment_to_faceCheckFragment"
            app:destination="@id/faceCheckFragment" />
    </fragment>

    <fragment
        android:id="@+id/faceCheckFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.FaceCheckFragment"
        android:label="fragment_face_check"
        tools:layout="@layout/face_check_fragment">
        <action
            android:id="@+id/action_faceCheckFragment_to_successFragment"
            app:destination="@id/successFragment" />
    </fragment>

    <fragment
        android:id="@+id/successFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.SuccessFragment"
        android:label="fragment_success"
        tools:layout="@layout/success_fragment" />

    <fragment
        android:id="@+id/chooseCountryFragment"
        android:name="com.vcheck.demo.dev.presentation.country_stage.ChooseCountryFragment"
        android:label="choose_country_fragment"
        tools:layout="@layout/choose_country_fragment">
        <argument
            android:name="countriesListTO"
            app:argType="com.vcheck.demo.dev.domain.CountriesListTO" />
        <action
            android:id="@+id/action_chooseCountryFragment_to_countryListFragment"
            app:destination="@id/countryListFragment">
            <argument
                android:name="countriesListTO"
                app:argType="com.vcheck.demo.dev.domain.CountriesListTO" />
        </action>
        <action
            android:id="@+id/action_chooseCountryFragment_to_chooseDocMethodScreen"
            app:destination="@id/chooseDocMethodScreen" />
    </fragment>

    <fragment
        android:id="@+id/chooseMethodScreen"
        android:name="com.vcheck.demo.dev.presentation.screens.ChooseMethodFragment"
        android:label="choose_method_screen"
        tools:layout="@layout/choose_method_fragment" />

    <fragment
        android:id="@+id/chooseDocMethodScreen"
        android:name="com.vcheck.demo.dev.presentation.doc_type_stage.ChooseDocMethodFragment"
        android:label="choose_doc_method_screen"
        tools:layout="@layout/choose_doc_method_fragment">
        <action
            android:id="@+id/action_chooseDocMethodScreen_to_photoInstructionsFragment"
            app:destination="@id/photoInstructionsFragment" />
    </fragment>

    <fragment
        android:id="@+id/photoUploadScreen"
        android:name="com.vcheck.demo.dev.presentation.photo_upload_stage.PhotoUploadFragment"
        android:label="photo_upload_screen"
        tools:layout="@layout/photo_upload_fragment">
        <action
            android:id="@+id/action_photoUploadScreen_to_checkPhotoFragment"
            app:destination="@id/checkPhotoFragment">
            <argument
                android:name="checkPhotoDataTO"
                app:argType="com.vcheck.demo.dev.presentation.transferrable_objects.CheckPhotoDataTO" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/inProcessFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.InProcessFragment"
        android:label="in_process_fragment"
        tools:layout="@layout/in_process_fragment" />

    <fragment
        android:id="@+id/frameInterferenceFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.FrameInterferenceFragment"
        android:label="frame_interference_fragment"
        tools:layout="@layout/frame_interference_fragment" />

    <fragment
        android:id="@+id/lookStraightErrorFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.LookStraightErrorFragment"
        android:label="look_straight_error_fragment"
        tools:layout="@layout/look_straight_error_fragment" />

    <fragment
        android:id="@+id/noTimeFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.NoTimeFragment"
        android:label="no_time_fragment"
        tools:layout="@layout/no_time_fragment" />

    <fragment
        android:id="@+id/photoInstructionsFragment"
        android:name="com.vcheck.demo.dev.presentation.photo_upload_stage.PhotoInstructionsFragment"
        android:label="photo_instructions_fragment"
        tools:layout="@layout/photo_instructions_fragment">
        <action
            android:id="@+id/action_photoInstructionsFragment_to_photoUploadScreen"
            app:destination="@id/photoUploadScreen" />
    </fragment>

    <fragment
        android:id="@+id/wrongMoveFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.WrongMoveFragment"
        android:label="wrong_move_fragment"
        tools:layout="@layout/wrong_move_fragment" />

    <fragment
        android:id="@+id/errorFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.ErrorFragment"
        android:label="error_fragment"
        tools:layout="@layout/error_fragment" />

    <fragment
        android:id="@+id/failVerificationFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.FailVerificationFragment"
        android:label="fail_verification_fragment"
        tools:layout="@layout/fail_verification_fragment" />

    <fragment
        android:id="@+id/noCameraFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.NoCameraFragment"
        android:label="no_camera_fragment"
        tools:layout="@layout/no_camera_fragment" />

    <fragment
        android:id="@+id/osNotSupportFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.OsNotSupportFragment"
        android:label="os_not_support_fragment"
        tools:layout="@layout/os_not_support_fragment" />

    <fragment
        android:id="@+id/notFoundFragment"
        android:name="com.vcheck.demo.dev.presentation.screens.NotFoundFragment"
        android:label="not_found_fragment"
        tools:layout="@layout/not_found_fragment" />

    <fragment
        android:id="@+id/checkPhotoFragment"
        android:name="com.vcheck.demo.dev.presentation.photo_upload_stage.CheckPhotoFragment"
        android:label="check_photo_fragment"
        tools:layout="@layout/check_photo_fragment">
        <argument
            android:name="checkPhotoDataTO"
            app:argType="com.vcheck.demo.dev.presentation.transferrable_objects.CheckPhotoDataTO" />
        <action
            android:id="@+id/action_checkPhotoFragment_to_checkInfoFragment"
            app:destination="@id/checkDocInfoFragment" />
    </fragment>

    <fragment
        android:id="@+id/checkDocInfoFragment"
        android:name="com.vcheck.demo.dev.presentation.check_doc_info_stage.CheckDocInfoFragment"
        android:label="check_doc_info_fragment"
        tools:layout="@layout/check_doc_info_fragment">
        <argument
            android:name="checkDocInfoDataTO"
            app:argType="com.vcheck.demo.dev.presentation.transferrable_objects.CheckDocInfoDataTO" />
        <action
            android:id="@+id/action_checkDocInfoFragment_to_livenessInstructionsFragment"
            app:destination="@id/livenessInstructionsFragment" />
    </fragment>

    <fragment
        android:id="@+id/countryListFragment"
        android:name="com.vcheck.demo.dev.presentation.country_stage.CountryListFragment"
        android:label="country_list_fragment"
        tools:layout="@layout/country_list_fragment">
        <argument
            android:name="countriesListTO"
            app:argType="com.vcheck.demo.dev.domain.CountriesListTO" />
    </fragment>

    <fragment
        android:id="@+id/demoStartFragment"
        android:name="com.vcheck.demo.dev.presentation.start.DemoStartFragment"
        android:label="fragment_demo_start"
        tools:layout="@layout/fragment_demo_start">
        <action
            android:id="@+id/action_demoStartFragment_to_livenessFragment2"
            app:destination="@id/livenessFragment2" />
        <action
            android:id="@+id/action_demoStartFragment_to_chooseCountryFragment"
            app:destination="@id/chooseCountryFragment">
            <argument
                android:name="countriesListTO"
                app:argType="com.vcheck.demo.dev.domain.CountriesListTO" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/livenessFragment2"
        android:name="com.vcheck.demo.dev.presentation.liveness.LivenessFragment"
        android:label="LivenessFragment" />

</navigation>